{
  "name": "@ticketmesh/bot",
  "version": "1.0.0",
  "description": "TicketMesh Discord Bot",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "deploy": "tsx src/deploy-commands.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "migrate": "tsx src/scripts/migrate.ts",
    "db:migrate": "tsx src/scripts/migrate.ts migrate",
    "db:seed": "tsx src/scripts/migrate.ts seed",
    "db:setup": "tsx src/scripts/migrate.ts setup",
    "db:test": "tsx src/scripts/migrate.ts test",
    "db:test-full": "tsx src/scripts/test-db.ts"
  },
  "dependencies": {
        "@discordjs/rest": "^2.6.0",
    "discord-api-types": "^0.38.26",
    "discord.js": "^14.14.1",
    "discraft": "^1.7.8",
    "dotenv": "^16.3.1",
    "glob": "^11.0.3",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "discord",
    "bot",
    "tickets"
  ],
  "author": "TicketMesh Team",
  "license": "MIT"
}